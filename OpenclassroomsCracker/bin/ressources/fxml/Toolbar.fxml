<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.text.Font?>

<ToolBar id="toolbar" prefHeight="78.0" prefWidth="745.0"
	stylesheets="@../css/toolbarStylesheets.css"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="com.aioschool.openclassroomscracker.ToolbarController">
	<items>
		<Label id="appLabel" fx:id="appLabel" text="OpenCracker">
			<font>
				<Font size="20.0" />
			</font>
		</Label>
		<Button id="homeButton" mnemonicParsing="false"
			onAction="#handleHomeButtonAction" prefHeight="59.0" text="Home" />
		<Button id="createButton" mnemonicParsing="false"
			onAction="#handleCreateButtonAction" prefHeight="58.0"
			prefWidth="106.0" text="Create" />
		<Button id="explorerButton" mnemonicParsing="false"
			onAction="#handleExplorerButtonAction" prefHeight="58.0"
			text="Explorer" />
		<Button id="gethelpButton" mnemonicParsing="false"
			onAction="#handleGethelpButtonAction" prefHeight="59.0"
			text="Get Help" />
	</items>
</ToolBar>
